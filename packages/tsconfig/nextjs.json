{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Next.js",
    "extends": "./base.json",
    "compilerOptions": {
        "lib": ["dom", "dom.iterable", "esnext"],
        "noEmit": true,
        "module": "esnext",
        "moduleResolution": "bundler",
        "jsx": "preserve",
        "plugins": [{ "name": "next" }],
        "incremental": true,
    }
}